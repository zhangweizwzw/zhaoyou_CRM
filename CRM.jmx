            <stringProp name="script">import org.json.JSONArray;
import org.json.JSONException;
import org.json.JSONObject;
import java.util.ArrayList;
import java.util.List;

String response_data = prev.getResponseDataAsString();
ArrayList idList = new ArrayList();
try {
  JSONObject data_obj = new JSONObject(response_data);
  String res=data_obj.getString(&quot;res&quot;);
  JSONObject res1=new JSONObject(res);

  /**
   * 新建客户-客户属性
   */
   String custAttr=res1.getString(&quot;customerAttrList&quot;);
   JSONArray customerAttrList=new JSONArray(custAttr);
   int custAttrIndex=(int)(Math.random()*customerAttrList.length());
//   log.error(&quot;客户属性:&quot;+customerAttrList.getJSONObject(custAttrIndex).getString(&quot;attrCode&quot;));
  

  /**
   * 签约-我方公司
   */
  String own=res1.getString(&quot;ownList&quot;);
  JSONArray ownList=new JSONArray(own);
  int ownIndex=(int)(Math.random()*customerAttrList.length());
//  log.error(&quot;我方公司:&quot;+ownList.getJSONObject(ownIndex).getString(&quot;ownName&quot;));

  /**
   * 签约-合同类型
   */
  String conType=res1.getString(&quot;conTypeList&quot;);
  JSONArray conTypeList=new JSONArray(conType);
  int conIndex=(int)(Math.random()*conTypeList.length());
//  log.error(&quot;合同类型:&quot;+conTypeList.getJSONObject(conIndex).getString(&quot;conType&quot;));

   /**
   * 签约-结算周期
   */
  String settleTerm=res1.getString(&quot;settleTermList&quot;);
  JSONArray settleTermList=new JSONArray(settleTerm);
  int settleIndex=(int)(Math.random()*settleTermList.length());
//  log.error(&quot;结算周期:&quot;+settleTermList.getJSONObject(settleIndex).getString(&quot;setType&quot;));

  /**
   * 拜访记录-跟进阶段
   */
  String trackStage=res1.getString(&quot;trackStageList&quot;);
  JSONArray trackStageList=new JSONArray(trackStage);
  int trackIndex=(int)(Math.random()*trackStageList.length());
//  log.error(&quot;跟进阶段:&quot;+trackStageList.getJSONObject(trackIndex).getString(&quot;trackStage&quot;));

  vars.put(&quot;attrCode&quot;,customerAttrList.getJSONObject(custAttrIndex).getString(&quot;attrCode&quot;));//新建客户-客户属性
  vars.put(&quot;ownId&quot;,ownList.getJSONObject(ownIndex).getString(&quot;ownId&quot;));//签约-我方公司id
  vars.put(&quot;ownName&quot;,ownList.getJSONObject(ownIndex).getString(&quot;ownName&quot;));//签约-我方公司name
  vars.put(&quot;conType&quot;,conTypeList.getJSONObject(conIndex).getString(&quot;conType&quot;));//签约-合同类型
  vars.put(&quot;setType&quot;,settleTermList.getJSONObject(settleIndex).getString(&quot;setType&quot;));//签约-结算周期
  vars.put(&quot;trackStage&quot;,trackStageList.getJSONObject(trackIndex).getString(&quot;trackStage&quot;));//拜访记录-跟进阶段
  
} catch (JSONException e) {
  e.printStackTrace();
}</stringProp>
